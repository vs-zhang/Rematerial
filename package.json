{
  "name": "RMD",
  "version": "0.1.0",
  "description": "React component wrappers around Google's Material Design Lite library",
  "author": "Vincent Zhang <vincentzhang1209@gmail.com> ",
  "contributors": [],
  "repository": "iceddev/react-material-design-lite",
  "license": "MIT",
  "engines": {
    "node": ">= 0.10"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ignore-styles test.setup.js src/**/*.spec.js",
    "build": "npm run clean && webpack --config webpack.config.babel.js",
    "clean": "rimraf index.js && rimraf app.css",
    "start": "nodemon --watch ./src --delay 250ms --exec 'npm run build && npm run dev:example'",
    "dev:example": "webpack-dev-server --config ./example/webpack.config.babel.js --no-info",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "material-design-icons": "^2.1.1",
    "material-design-lite": "1.0.0-3"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "@phated/eslint-config-iceddev": "^0.2.1",
    "babel": "^5.6.14",
    "babel-core": "^5.6.20",
    "babel-loader": "^5.3.1",
    "css-loader": "^0.15.3",
    "eslint": "^1.10.3",
    "eslint-plugin-mocha": "^0.5.1",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "sinon": "^1.17.4",
    "babel-register": "^6.4.3",
    "react-addons-test-utils": "^0.14.7",
    "babel-preset-airbnb": "^1.0.1"
  },
  "keywords": [
    "material",
    "material design",
    "mdl",
    "react"
  ]
}
